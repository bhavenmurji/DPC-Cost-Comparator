{
  "name": "@health-partnership-x/api",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "lint": "eslint . --ext ts --report-unused-disable-directives --max-warnings 0",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "scrape:dpc": "tsx ../../scripts/scrape-dpc-providers.ts",
    "scrape:dpc:test": "tsx ../../scripts/scrape-dpc-providers.ts --limit 10",
    "scrape:dpca": "tsx ../../scripts/scrape-dpca-directory.ts",
    "scrape:dpca:test": "tsx ../../scripts/scrape-dpca-directory.ts --limit 10",
    "match:dpca": "tsx ../../scripts/match-dpca-providers.ts",
    "match:dpca:report": "tsx ../../scripts/match-dpca-providers.ts --report",
    "scrape:fees": "tsx ../../scripts/scrape-practice-fees.ts",
    "scrape:fees:report": "tsx ../../scripts/scrape-practice-fees.ts --report",
    "pricing:save": "tsx ../../scripts/save-scraped-pricing.ts",
    "pricing:scrape": "tsx ../../scripts/scrape-practice-pricing.ts",
    "pricing:scrape:test": "tsx ../../scripts/scrape-practice-pricing.ts --limit 10",
    "import:walmart": "tsx ../../scripts/import-walmart-programs.ts",
    "enrich:providers": "tsx ../../scripts/enrich-providers-npi.ts",
    "enrich:providers:test": "tsx ../../scripts/enrich-providers-npi.ts --limit 10"
  },
  "dependencies": {
    "@prisma/client": "^5.10.2",
    "@sendgrid/mail": "^8.1.6",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.4.4",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "playwright": "^1.56.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.20",
    "prisma": "^5.10.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  }
}
